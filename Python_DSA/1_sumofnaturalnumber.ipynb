   "source": [
    "# Print Name N times using Recursion\n",
    "# 1 ≤ N ≤ 1000\n",
    "# You must use recursion (not loops!)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "id": "59c13e97",
   "metadata": {},
   "outputs": [],
   "source": [
    "def print_name(n, name):\n",
    "    print(name  if n == 1 else print_name(n - 1, name))\n",
    "    return name\n"
   ]
  },
